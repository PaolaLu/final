{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport custom_axios from '../plugins/axios.js';\nimport AbmReservaAula from './AbmReservaAula.vue';\nexport default {\n  components: {\n    AbmReservaAula\n  },\n  data() {\n    return {\n      headers: [{\n        text: 'Aula',\n        value: 'aula.descripcion'\n      }, {\n        text: 'Fecha y hora desde',\n        value: 'fh_desde'\n      }, {\n        text: 'Fecha y hora hasta',\n        value: 'fh_hasta'\n      }, {\n        text: 'Observación',\n        value: 'observacion'\n      }, {\n        text: 'Acciones',\n        value: 'actions',\n        sortable: false\n      }],\n      listadoReserva: [],\n      mostrarAbmReserva: false,\n      mostrarConfirmacion: false,\n      reservaSeleccionada: {},\n      reservaEliminar: null,\n      editar: false\n    };\n  },\n  methods: {\n    todasLasReservas() {\n      custom_axios.get('/apiv1/reservaaula').then(response => {\n        if (response.status === 200) {\n          this.listadoReserva = response.data;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    navigateToAbmReserva() {\n      this.$router.push('/abmreservaaula');\n    },\n    navigateToHome() {\n      this.$router.push('/');\n    },\n    editarReserva(item) {\n      this.reservaSeleccionada = {\n        ...item\n      };\n      this.editar = true;\n      this.mostrarAbmReserva = true;\n    },\n    confirmarEliminarReserva(item) {\n      this.reservaEliminar = item;\n      this.mostrarConfirmacion = true;\n    },\n    eliminarReservaConfirmada() {\n      custom_axios.delete(`/apiv1/reservaaula/${this.reservaEliminar.id}`).then(response => {\n        if (response.status === 204) {\n          this.todasLasReservas();\n          this.mostrarConfirmacion = false;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    guardarAbmReserva() {\n      this.mostrarAbmReserva = false;\n      this.todasLasReservas();\n    },